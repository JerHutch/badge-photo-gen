{
  "name": "badge-gen",
  "version": "1.0.0",
  "description": "CLI tool to generate caricature-style employee badge photos using AI for test data",
  "main": "dist/index.js",
  "bin": {
    "badge-gen": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "badge",
    "photo",
    "generator",
    "ai",
    "test-data",
    "stability-ai",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "yaml": "^2.3.4",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "sharp": "^0.33.1",
    "cli-progress": "^3.12.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/cli-progress": "^3.11.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "vitest": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
